{
	
	"AWSEBDockerrunVersion": 2,
	"containerDefinations": [

      {
        "name": "client",
        "image": "chauhany/multi-client",
        "hostname": "client",
        "essential": "false"

         },
         {
         "name": "server",
         "image": "chauhany/multi-server",
         "hostname": "api",
         "essential": "false"
         },
         {
          "name": "worker",
         "image": "chauhany/multi-worker",
         "hostname": "worker",
         "essential": "false"
         },
         {
         "name": "nginx",
         "image": "chauhany/multi-nginx",
         "hostname": "nginx",
         "essential": "true",
         "portMappings": [
         {
         	"hostPort": 80,
         	"containerPort": 80

         }


         ],

         "links": ["client", "server"]

         }



	]

}
